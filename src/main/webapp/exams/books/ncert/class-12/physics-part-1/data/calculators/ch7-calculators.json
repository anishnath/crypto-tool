{
  "q7-1-resistive-ac": {
    "title": "AC Circuit with Pure Resistor",
    "formula_display": "I = V/R, P = V\u00b2/R",
    "inputs": [
      { "id": "V", "label": "RMS Voltage V", "unit": "V", "default": 220, "min": 1, "max": 10000, "step": 1 },
      { "id": "R", "label": "Resistance R", "unit": "\u03a9", "default": 100, "min": 1, "max": 100000, "step": 1 }
    ],
    "compute": "V * V / R",
    "output": { "label": "Power P", "unit": "W", "precision": 4 },
    "plot": {
      "vary": "R",
      "range": [10, 500],
      "points": 60,
      "xLabel": "Resistance R (\u03a9)",
      "yLabel": "Power P (W)"
    }
  },

  "q7-2-peak-rms": {
    "title": "Peak and RMS Values",
    "formula_display": "V_rms = V\u2080/\u221a2",
    "inputs": [
      { "id": "V0", "label": "Peak voltage V\u2080", "unit": "V", "default": 300, "min": 1, "max": 10000, "step": 1 }
    ],
    "compute": "V0 / 1.41421",
    "output": { "label": "RMS voltage V_rms", "unit": "V", "precision": 4 },
    "plot": {
      "vary": "V0",
      "range": [10, 1000],
      "points": 60,
      "xLabel": "Peak voltage V\u2080 (V)",
      "yLabel": "V_rms (V)"
    }
  },

  "q7-3-inductive-reactance": {
    "title": "AC Circuit with Pure Inductor",
    "formula_display": "X_L = 2\u03c0fL, I = V/X_L",
    "inputs": [
      { "id": "V", "label": "RMS Voltage V", "unit": "V", "default": 220, "min": 1, "max": 10000, "step": 1 },
      { "id": "f", "label": "Frequency f", "unit": "Hz", "default": 50, "min": 1, "max": 10000, "step": 1 },
      { "id": "L", "label": "Inductance L", "unit": "H", "default": 0.044, "min": 0.001, "max": 100, "step": 0.001 }
    ],
    "compute": "V / (2 * 3.14159 * f * L)",
    "output": { "label": "RMS current I", "unit": "A", "precision": 4 },
    "plot": {
      "vary": "f",
      "range": [10, 500],
      "points": 60,
      "xLabel": "Frequency f (Hz)",
      "yLabel": "I (A)"
    }
  },

  "q7-4-capacitive-reactance": {
    "title": "AC Circuit with Pure Capacitor",
    "formula_display": "X_C = 1/(2\u03c0fC), I = V/X_C",
    "inputs": [
      { "id": "V", "label": "RMS Voltage V", "unit": "V", "default": 110, "min": 1, "max": 10000, "step": 1 },
      { "id": "f", "label": "Frequency f", "unit": "Hz", "default": 60, "min": 1, "max": 10000, "step": 1 },
      { "id": "C", "label": "Capacitance C", "unit": "\u03bcF", "default": 60, "min": 0.1, "max": 100000, "step": 0.1 }
    ],
    "compute": "V * 2 * 3.14159 * f * C * 1e-6",
    "output": { "label": "RMS current I", "unit": "A", "precision": 4 },
    "plot": {
      "vary": "f",
      "range": [10, 500],
      "points": 60,
      "xLabel": "Frequency f (Hz)",
      "yLabel": "I (A)"
    }
  },

  "q7-6-lc-frequency": {
    "title": "LC Oscillation Frequency",
    "formula_display": "\u03c9 = 1/\u221a(LC)",
    "inputs": [
      { "id": "L", "label": "Inductance L", "unit": "mH", "default": 27, "min": 0.1, "max": 10000, "step": 0.1 },
      { "id": "C", "label": "Capacitance C", "unit": "\u03bcF", "default": 30, "min": 0.1, "max": 100000, "step": 0.1 }
    ],
    "compute": "1 / Math.sqrt(L * 1e-3 * C * 1e-6)",
    "output": { "label": "Angular frequency \u03c9", "unit": "rad/s", "precision": 4 },
    "plot": {
      "vary": "C",
      "range": [5, 200],
      "points": 60,
      "xLabel": "Capacitance C (\u03bcF)",
      "yLabel": "\u03c9 (rad/s)"
    }
  },

  "q7-7-resonance-power": {
    "title": "Power at Resonance (Series LCR)",
    "formula_display": "P = V\u00b2/R (at resonance Z = R)",
    "inputs": [
      { "id": "V", "label": "RMS Voltage V", "unit": "V", "default": 200, "min": 1, "max": 10000, "step": 1 },
      { "id": "R", "label": "Resistance R", "unit": "\u03a9", "default": 20, "min": 1, "max": 10000, "step": 1 }
    ],
    "compute": "V * V / R",
    "output": { "label": "Average power P", "unit": "W", "precision": 4 },
    "plot": {
      "vary": "R",
      "range": [5, 200],
      "points": 60,
      "xLabel": "Resistance R (\u03a9)",
      "yLabel": "P (W)"
    }
  },

  "q7-8-lcr-impedance": {
    "title": "Series LCR Circuit Impedance",
    "formula_display": "Z = \u221a(R\u00b2 + (X_L \u2212 X_C)\u00b2)",
    "inputs": [
      { "id": "R", "label": "Resistance R", "unit": "\u03a9", "default": 40, "min": 1, "max": 10000, "step": 1 },
      { "id": "L", "label": "Inductance L", "unit": "H", "default": 5.0, "min": 0.01, "max": 100, "step": 0.01 },
      { "id": "C", "label": "Capacitance C", "unit": "\u03bcF", "default": 80, "min": 0.1, "max": 100000, "step": 0.1 },
      { "id": "f", "label": "Frequency f", "unit": "Hz", "default": 7.96, "min": 0.1, "max": 10000, "step": 0.01 }
    ],
    "compute": "Math.sqrt(R*R + Math.pow(2*3.14159*f*L - 1/(2*3.14159*f*C*1e-6), 2))",
    "output": { "label": "Impedance Z", "unit": "\u03a9", "precision": 4 },
    "plot": {
      "vary": "f",
      "range": [1, 30],
      "points": 100,
      "xLabel": "Frequency f (Hz)",
      "yLabel": "Z (\u03a9)"
    }
  }
}
