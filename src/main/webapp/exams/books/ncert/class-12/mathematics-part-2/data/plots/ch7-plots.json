    ],
    "annotations": [
      { "x": 0, "text": "f(0) = 0, F(0) = 0" },
      { "x": 1, "text": "f(1) = 1, F(1) = ln 2" },
      { "x": -1, "text": "f(-1) = -1" }
    ]
  },
  "ex-7-2-q5": {
