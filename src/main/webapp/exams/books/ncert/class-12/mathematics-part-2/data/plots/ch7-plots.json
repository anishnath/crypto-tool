{
  "ex-7-1-q1": {
    "title": "Integral of sin(2x): antiderivative F(x) = -cos(2x)/2",
    "xDomain": [-6.5, 6.5],
    "yDomain": [-1.5, 1.5],
    "data": [
      { "fn": "sin(2*x)", "color": "#6366f1", "label": "f(x) = sin(2x)" },
      { "fn": "-cos(2*x)/2", "derivative": { "fn": "sin(2*x)", "updateOnMouseMove": true }, "color": "#22c55e", "label": "F(x) = -cos(2x)/2" }
    ],
    "annotations": [
      { "x": 0, "text": "F(0) = -1/2" },
      { "x": 1.5708, "text": "f = 0, F = max at \u03c0/2" },
      { "x": 0.7854, "text": "f = max, F = 0 at \u03c0/4" }
    ]
  },
  "ex-7-1-q5": {
    "title": "Integral of (sin 2x - 4e^(3x)): antiderivative F(x)",
    "xDomain": [-2, 2],
    "yDomain": [-30, 5],
    "data": [
      { "fn": "sin(2*x) - 4*exp(3*x)", "color": "#6366f1", "label": "f(x) = sin(2x) - 4e^(3x)" },
      { "fn": "-cos(2*x)/2 - (4/3)*exp(3*x)", "derivative": { "fn": "sin(2*x) - 4*exp(3*x)", "updateOnMouseMove": true }, "color": "#22c55e", "label": "F(x) = -cos(2x)/2 - (4/3)e^(3x)" }
    ],
    "annotations": [
      { "x": 0, "text": "f(0) = -4, F(0) = -11/6" },
      { "x": 1, "text": "Exponential dominates" }
    ]
  },
  "ex-7-2-q1": {
