				<h5 class="h6">~/.ssh/config Example</h5>
				<pre class="bg-light p-3 border rounded"><code># GitHub configuration
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_ed25519
    IdentitiesOnly yes

# Production server
Host prod-server
    HostName 192.168.1.100
    User admin
    Port 22
    IdentityFile ~/.ssh/prod_key
    ServerAliveInterval 60
    ServerAliveCountMax 3

# Jump host / Bastion
Host internal-server
    HostName 10.0.0.50
    User developer
    ProxyJump bastion.example.com
    IdentityFile ~/.ssh/id_ed25519

# Global defaults
Host *
    AddKeysToAgent yes
    UseKeychain yes
    IdentityFile ~/.ssh/id_ed25519
    IdentityFile ~/.ssh/id_rsa</code></pre>

