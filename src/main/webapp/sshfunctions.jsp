					// Submit form
					$('#form').submit();

					// Close modal after short delay
					setTimeout(function() {
						$('#emailModal').modal('hide');
						$('#sendEmailBtn').prop('disabled', false).html('<i class="fas fa-paper-plane"></i> Send Keys');

						// Show success message
						$('#output').prepend('<div class="alert alert-info alert-dismissible fade show" role="alert">' +
							'<i class="fas fa-envelope"></i> <strong>Email Sent!</strong> Your SSH keys are being sent to ' + email + '.' +
